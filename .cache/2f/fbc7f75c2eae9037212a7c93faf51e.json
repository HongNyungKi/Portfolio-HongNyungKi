ight-semi-bold:600;\r\n$weight-regular:400;\r\n\r\n* {\r\n    box-sizing: border-box;\r\n}\r\n\r\nbody {\r\n    font-family: 'Open Sans', sans-serif;\r\n    cursor: default;\r\n}\r\n\r\na {\r\n    text-decoration: none;\r\n    color: $white;\r\n}\r\n\r\nbutton {\r\n    background: transparent;\r\n    border: none;\r\n    cursor: pointer;\r\n    outline: none;\r\n}\r\n\r\n.section {\r\n    text-align: center;\r\n    padding: 50px;\r\n    max-width: 1200px;\r\n    margin: auto;\r\n}\r\n\r\n//TYPOGRAPHY\r\nh1 {\r\n    font-size: $font-large;\r\n    font-weight: $weight-bold;\r\n    color: $black;\r\n    margin: 16px 0;\r\n}\r\n\r\nh2 {\r\n    font-size: $font-regular;\r\n    font-weight: $weight-semi-bold;\r\n    color: $black;\r\n    margin: 8px 0;\r\n}\r\n\r\nh3 {\r\n    font-size: $font-regular;\r\n    font-weight: $weight-regular;\r\n    color: $black;\r\n    margin: 8px 0;\r\n}\r\n\r\np {\r\n    font-size: $font-regular;\r\n    font-weight: $weight-regular;\r\n    color: $black;\r\n    margin: 4px 0;\r\n}\r\n\r\n//NAVBAR\r\n#navbar {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    background: $skyBlue;\r\n    padding: 18px;\r\n    color: white;\r\n\r\n    .navbar__logo {\r\n        display: flex;\r\n        align-items: center;\r\n        font-size: $font-medium;\r\n        font-weight: $weight-semi-bold;\r\n        letter-spacing: 4px;\r\n        margin-left: 10px;\r\n    }\r\n\r\n    .navbar__menu {\r\n        display: flex;\r\n\r\n        .navbar__menu__item {\r\n            padding: 10px;\r\n            margin: 0 10px;\r\n            font-size: $font-regular;\r\n            cursor: pointer;\r\n            border-radius: 4px;\r\n\r\n            &:hover {\r\n                background: $indigo;\r\n            }\r\n\r\n            &.active {\r\n                border: 1px solid $white;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n//SECTION-HOME\r\n#home {\r\n    background: url('./img/background.png') center/cover no-repeat;\r\n    text-align: center;\r\n    padding: 80px 0;\r\n\r\n    .home__avatar {\r\n        width: 250px;\r\n        height: 250px;\r\n        background: $white;\r\n        border-radius: 50%;\r\n        object-fit: contain;\r\n        background: white;\r\n    }\r\n\r\n    .home__title,\r\n    .home__description {\r\n        color: $white;\r\n    }\r\n\r\n    .home__contact {\r\n        color: $white;\r\n        font-size: $font-regular;\r\n        font-weight: $weight-semi-bold;\r\n        margin-top: 24px;\r\n        padding: 8px 12px;\r\n        border: 2px solid $white;\r\n        border-radius: 4px;\r\n    }\r\n}\r\n\r\n//SECTION-ABOUT\r\n.about__majors {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    margin: 80px 0;\r\n\r\n    .major {\r\n        img {\r\n            width: 200px;\r\n            height: 200px;\r\n            border: 1px solid $blue;\r\n            border-radius: 50%;\r\n            padding: 20px;\r\n            margin-bottom: 16px;\r\n            transition: all 100ms ease-in;\r\n        }\r\n\r\n        .major__description {\r\n            color: gray;\r\n            font-size: $font-small;\r\n        }\r\n\r\n        &:hover img {\r\n            padding: 7px;\r\n            border: 3px solid $indigo;\r\n        }\r\n\r\n        &:hover .major__description {\r\n            color: $black;\r\n        }\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"4c1316a7948f37d182cfa1bd6671802e","cacheData":{"env":{}}}