r: none;\r\n    cursor: pointer;\r\n    outline: none;\r\n}\r\n\r\n.section {\r\n    text-align: center;\r\n    padding: 50px;\r\n    margin: auto;\r\n}\r\n\r\n.section__container {\r\n    max-width: 1200px;\r\n    margin: auto;\r\n}\r\n\r\n//TYPOGRAPHY\r\nh1 {\r\n    font-size: $font-large;\r\n    font-weight: $weight-bold;\r\n    color: $black;\r\n    margin: 16px 0;\r\n}\r\n\r\nh2 {\r\n    font-size: $font-regular;\r\n    font-weight: $weight-semi-bold;\r\n    color: $black;\r\n    margin: 8px 0;\r\n}\r\n\r\nh3 {\r\n    font-size: $font-regular;\r\n    font-weight: $weight-regular;\r\n    color: $black;\r\n    margin: 8px 0;\r\n}\r\n\r\np {\r\n    font-size: $font-regular;\r\n    font-weight: $weight-regular;\r\n    color: $black;\r\n    margin: 4px 0;\r\n}\r\n\r\n//NAVBAR\r\n#navbar {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    background: $skyBlue;\r\n    padding: 18px;\r\n    color: white;\r\n\r\n    .navbar__logo {\r\n        display: flex;\r\n        align-items: center;\r\n        font-size: $font-medium;\r\n        font-weight: $weight-semi-bold;\r\n        letter-spacing: 4px;\r\n        margin-left: 10px;\r\n    }\r\n\r\n    .navbar__menu {\r\n        display: flex;\r\n\r\n        .navbar__menu__item {\r\n            padding: 10px;\r\n            margin: 0 10px;\r\n            font-size: $font-regular;\r\n            cursor: pointer;\r\n            border-radius: 4px;\r\n\r\n            &:hover {\r\n                background: $indigo;\r\n            }\r\n\r\n            &.active {\r\n                border: 1px solid $white;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n//SECTION-HOME\r\n#home {\r\n    background: url('./img/background.png') center/cover no-repeat;\r\n    text-align: center;\r\n    padding: 80px 0;\r\n\r\n    .home__avatar {\r\n        width: 250px;\r\n        height: 250px;\r\n        background: $white;\r\n        border-radius: 50%;\r\n        object-fit: contain;\r\n        background: white;\r\n    }\r\n\r\n    .home__title,\r\n    .home__description {\r\n        color: $white;\r\n    }\r\n\r\n    .home__contact {\r\n        color: $white;\r\n        font-size: $font-regular;\r\n        font-weight: $weight-semi-bold;\r\n        margin-top: 24px;\r\n        padding: 8px 12px;\r\n        border: 2px solid $white;\r\n        border-radius: 4px;\r\n    }\r\n}\r\n\r\n//SECTION-ABOUT\r\n.about__majors {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    margin: 80px 0;\r\n\r\n    .major {\r\n        img {\r\n            width: 200px;\r\n            height: 200px;\r\n            border: 1px solid $blue;\r\n            border-radius: 50%;\r\n            padding: 20px;\r\n            margin-bottom: 16px;\r\n            transition: all 100ms ease-in;\r\n        }\r\n\r\n        .major__description {\r\n            color: gray;\r\n            font-size: $font-small;\r\n        }\r\n\r\n        &:hover img {\r\n            padding: 7px;\r\n            border: 3px solid $indigo;\r\n        }\r\n\r\n        &:hover .major__description {\r\n            color: $black;\r\n        }\r\n    }\r\n}\r\n\r\n//SECTION-SKILLS\r\n#skills {\r\n    background: $white;\r\n\r\n    .skillset {\r\n        display: flex;\r\n        background: $skyBlue;\r\n        margin: 20px 0;\r\n\r\n        .skillset__left {\r\n            flex-basis: 60%;\r\n            padding: 20px 40px;\r\n\r\n            .skill {\r\n                .skill__description {\r\n                    display: flex;\r\n                    justify-content: space-between;\r\n                }\r\n\r\n                .skill__bar {\r\n                    width: 100%;\r\n                    height: 4px;\r\n                    background: $white;\r\n                    margin-bottom: 32px;\r\n\r\n                    .skill__value {\r\n                        width: 70%;\r\n                        height: 4px;\r\n                        background: $red;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n\r\n        .skillset__right {\r\n            flex-basis: 40%;\r\n            background: $blue;\r\n            color: $white;\r\n            display: flex;\r\n            flex-direction: column;\r\n            justify-content: space-between;\r\n\r\n            .skillset__title {\r\n                color: $white;\r\n            }\r\n\r\n            .tools {\r\n                flex-basis: 70%;\r\n                padding: 20px;\r\n            }\r\n\r\n            .etc {\r\n                background: $indigo;\r\n                flex-basis: 30%;\r\n                padding: 20px;\r\n            }\r\n        }\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"0f7eeb36e75a96b38cbf1554d89374e8","cacheData":{"env":{}}}